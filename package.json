{
  "version": "1.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": "github:grimmer0125/cli-open-dicom-with-chrome",
  "bin": {
    "dicom": "./dist/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "deploy": "node -r ts-node/register/transpile-only  -r tsconfig-paths/register dist/index.js",
    "deploy2": "TS_NODE_PROJECT=tsconfig.prod.json node  -r tsconfig-paths/register dist/index.js",
    "deploy3": "cp tsconfig.json dist/ && TS_NODE_PROJECT=dist/tsconfig.json node  -r tsconfig-paths/register dist/index.js",
    "prepare": "tsc"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "cli-open-dicom-with-chrome",
  "author": "Grimmer",
  "devDependencies": {
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/node": "^14.0.14",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2"
  }
}
